ies = await JoblyApi.getCompanies(name);\n    setCompanies(companies);\n  }\n\n  if (!currentUser) {\n    return (\n      <div>\n        <br />\n        <br />\n        <h4 className=\"unauthorized\">\n          Please log in or create an account to view companies\n        </h4>\n      </div>\n    );\n  }\n\n  if (!filteredCompanies) return <LoadingSpinner />;\n\n  return (\n    <div>\n      <br />\n      <h1>List of Companies</h1>\n      <br />\n      <div className=\"SearchForm mb-4\">\n        <form className=\"form-inline\" onSubmit={handleSubmit}>\n          <input\n            className=\"form-control form-control-lg flex-grow-1\"\n            name=\"searchTerm\"\n            placeholder=\"Enter search term\"\n            value={searchTerm}\n            onChange={handleChange}\n          />\n        </form>\n      </div>{\" \"}\n      {filteredCompanies.length ? (\n        <div>\n          {filteredCompanies.map((c) => (\n            <CompanyCard\n              key={c.handle}\n              handle={c.handle}\n              name={c.name}\n              description={c.description}\n              logoUrl={c.logoUrl}\n            />\n          ))}\n        </div>\n      ) : (\n        <p>No results found</p>\n      )}\n    </div>\n  );\n}\n\nexport default Companies;\n","C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\jobly\\jobly-frontend\\src\\components\\CompanyComponents\\Company.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\jobly\\jobly-frontend\\src\\components\\JobComponents\\Jobs.js",["84","85"],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\jobly\\jobly-frontend\\src\\components\\Common\\Alert.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\jobly\\jobly-frontend\\src\\components\\Common\\LoadingSpinner.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\j